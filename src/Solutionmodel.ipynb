    for b in range(len(input2[0])):
        for rgb in range(3):
            pred = input1[a][b][rgb]
            og = input2[a][b][rgb]
            if og > (255 - og) :
                mdev = og
            else: 
                mdev = 255 - og
            pdev = og - pred 
            pdev = abs(pdev)
            acc = ((pdev/mdev)*100)
            means.append(acc)
print(means)

