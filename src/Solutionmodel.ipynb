# Mathemaical analysis

def load_images_from_folder(folder):
    images = []
    filenames = []
    for filename in os.listdir(folder):
        filenames.append(filename)
    filenames.sort()
    for filename in filenames:
        img = cv2.imread(os.path.join(folder,filename))
        if img is not None:
            images.append(img)
    return images
    
# loads images from specified folder and puts the images in order

wv = load_images_from_folder("Water Vapour")
wv = np.array(wv)

cd = load_images_from_folder("Carbon Dioxide")
cd = np.array(cd)

aod = load_images_from_folder("Aerosol Optical Depth")
aod = np.array(aod)

oz = load_images_from_folder("Ozone")
oz = np.array(oz)

cf = load_images_from_folder("CF")
cf = np.array(cf)

print(len(wv))

# imports all of the images from  our 5 folders and converts them to numpy arrays

new = []

length = len(wv[0])
breadth = len(wv[0][0])

# assigns the values of image dimensions to variables

for rgb in range(3):
    for breadth_pixel in range(breadth):
        for length_pixel in range(length):
            x = []
            y = []
            z = []
            a = []
            b = []
            c = []
            for image_number in range(len(wv)):
                x.append(image_number)
                y.append(wv[image_number][length_pixel][breadth_pixel][rgb])
                z.append(cd[image_number][length_pixel][breadth_pixel][rgb])
                a.append(aod[image_number][length_pixel][breadth_pixel][rgb])
                b.append(oz[image_number][length_pixel][breadth_pixel][rgb])
                c.append(cf[image_number][length_pixel][breadth_pixel][rgb])
            nx = len(wv)
            one = np.polyfit(x , y, 1)
            two = np.polyfit(x , z, 1)
            three = np.polyfit(x , a, 1)
            four = np.polyfit(x , b, 1)
            five = np.polyfit(x , c, 1)
            
            none = one[0] * nx + one[1]
            ntwo = two[0] * nx + two[1]
            nthree = three[0] * nx + three[1]
            nfour = four[0] * nx + four[1]
            nfive = five[0] * nx + five[1]
            
            ny = (8*none + 2*ntwo + 7*nthree + 5*nfour + 3*nfive)/25
            new.append(ny)
            #plt.plot(nx,ny,marker = "o")
#plt.savefig('image3.eps', format='eps')
#plt.show()
    
# This is our multivariate regression model, which predicts the combined map, and individual predictions of all other aps

for pixel in range(len(new)):
    if new[pixel] < 0 :
        new[pixel] = 0
    elif new[pixel] > 255 :
        new[pixel] = 255
        
# Clamping of all value to prevent glitching

new_image = []
for breadth_pixel in range(breadth): 
    x = []
    for length_pixel in range(length):
        x.append([new[(2*length*breadth)+((breadth_pixel*length) + length_pixel)],
                  new[(1*length*breadth)+((breadth_pixel*length) + length_pixel)],
                  new[(0*length*breadth)+((breadth_pixel*length) + length_pixel)]])
    new_image.append(x)

# converts the array 'new' into a workable array in the form of initial encoding

new_image = np.array(new_image)

# converts 'new_image' array to a NumPy array

def output_image(img):
    raw = Image.fromarray(img.astype(np.uint8), 'RGB')
    final_image = raw.transpose(Image.FLIP_LEFT_RIGHT)
    final_image.save("Output_Image.png")
    
# function to output raw final image and save it.

output_image(new_image)

